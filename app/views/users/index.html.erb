<%= will_paginate %>
<ul class="users">
  <% @users.each do |user| %>
    <li>
      <%= link_to "#{user.first_name} #{user.last_name}", user %>
      | <%= link_to "delete", user, :method => :delete, :confirm => "You sure?",
                             		  :title => "Delete #{user.first_name} #{user.last_name}" %>
    </li>
  <% end %>
</ul>
